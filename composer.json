{
    "name": "jiny/social",
    "description": "jiny social auth for JinyPHP",
    "keywords": [
        "jinyPHP",
        "framwork",
        "social"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "livewire/livewire": "^2.10",

        "laravel/socialite": "^5.5",
        "socialiteproviders/kakao": "^4.2",
        "socialiteproviders/naver": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Social\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Social\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Social\\JinySocialServiceProvider"
            ]
        }
    }
}
